{
  "name": "yorkshiredigital",
  "description": "Bringing Yorkshire's digital community together.",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YorkshireDigital/yorkshiredigital.com.git"
  },
  "homepage": "https://github.com/YorkshireDigital/yorkshiredigital.com",
  "keywords": [
  ],
  "main": "dist/server.js",
  "scripts": {
    "test": "",
    "start": "forever --minUptime 1000 --spinSleepTime 1000 -c \"node --harmony\" ./dist/server.js",
    "build-server": "webpack --colors --display-error-details --config configs/webpack.server.js",
    "build-client": "webpack --colors --display-error-details --config configs/webpack.client.js",
    "build": "concurrently \"npm run build-server\" \"npm run build-client\"",
    "watch-server": "webpack --watch --verbose --colors --display-error-details --config configs/webpack.server-watch.js",
    "watch-server-start": "node node_modules/just-wait --pattern \"dist/*.js\" && npm run start",
    "watch-client": "webpack-dev-server --config configs/webpack.client-watch.js",
    "production": "cross-env NODE_ENV=production npm run build && cross-env NODE_ENV=production npm run start",
    "dev": "concurrently --kill-others \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\"",
    "postinstall": "webpack -p --config configs/webpack.client.js"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "babel": "6.5.2",
    "babel-core": "6.7.2",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.7.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "bcryptjs": "^2.3.0",
    "bell": "^7.3.0",
    "boom": "^3.1.2",
    "cross-env": "^1.0.7",
    "file-loader": "^0.8.5",
    "forever": "0.15.1",
    "h2o2": "^5.1.0",
    "hapi": "^13.2.1",
    "hapi-auth-cookie": "^6.1.1",
    "hapi-auth-jwt2": "^5.4.1",
    "hapi-sequelize": "^2.2.4",
    "inert": "^3.2.0",
    "jsonwebtoken": "^5.7.0",
    "lodash": "^4.6.1",
    "material-ui": "^0.15.0-alpha.2",
    "mysql": "^2.10.2",
    "piping": "0.3.0",
    "radium": "^0.16.6",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.3.1",
    "redux-form": "^4.2.2",
    "redux-localstorage": "^0.4.0",
    "redux-thunk": "^2.0.1",
    "sequelize": "^3.20.0",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.0",
    "concurrently": "^2.0.0",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.3.0",
    "eslint-plugin-react": "^4.2.1",
    "json-loader": "^0.5.4",
    "just-wait": "1.0.5",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-slider-monitor": "^1.0.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-node-externals": "^1.0.0"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
